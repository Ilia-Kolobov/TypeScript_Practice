{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "allowJs": true,

    "strict": true,
    "esModuleInterop": true,  
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
  , "exclude":["node_modules"]
}